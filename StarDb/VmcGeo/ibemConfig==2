   IBEM->AddNode(IBSS,1,new TGeoCombiTrans(0,0,-128.75,GetRot("90YX")));
   IBEM->AddNode(IBSS,2,new TGeoCombiTrans(0,0,126.845,GetRot("90YX")));
  CAVE->AddNode(IBEM,2,new TGeoCombiTrans(0,-16.51,-542.355,GetRot("180R")));
  CAVE->AddNode(IBSA,1,new TGeoTranslation(24.11,-60.1,692.91));
   IBSA->AddNode(IBAA,1,new TGeoTranslation(0,0,-3.335));
   IBSA->AddNode(IBAA,2,new TGeoCombiTrans(-3.335,0,0,GetRot("90YX")));
  CAVE->AddNode(IBSA,2,new TGeoCombiTrans(-24.11,-60.1,692.91,GetRot("R180")));
  CAVE->AddNode(IBSA,3,new TGeoCombiTrans(24.11,-60.1,-694.815,GetRot("180R")));
  CAVE->AddNode(IBSA,4,new TGeoCombiTrans(-24.11,-60.1,-694.815,GetRot("180T")));
  rot = new TGeoRotation("next",90,0,135,90,45,90);
  CAVE->AddNode(IBSB,1,new TGeoCombiTrans(24.11,-69.5,631.2,rot));
   IBSB->AddNode(IBAB,1,new TGeoTranslation(0,0,3.335));
   IBSB->AddNode(IBAB,2,new TGeoCombiTrans(-3.335,0,0,GetRot("90YX")));
  rot = new TGeoRotation("next",270,0,135,90,45,90);
  CAVE->AddNode(IBSB,2,new TGeoCombiTrans(-24.11,-69.5,631.2,rot));
  rot = new TGeoRotation("next",90,0,45,90,135,90);
  CAVE->AddNode(IBSB,3,new TGeoCombiTrans(24.11,-69.5,-633.105,rot));
  rot = new TGeoRotation("next",270,0,45,90,135,90);
  CAVE->AddNode(IBSB,4,new TGeoCombiTrans(-24.11,-69.5,-633.105,rot));
  CAVE->AddNode(IBSC,1,new TGeoTranslation(0,-23.6,581.1));
   IBSC->AddNode(IBAC,1,new TGeoTranslation(0,0,4.76));
   IBSC->AddNode(IBAC,2,new TGeoTranslation(0,0,-4.76));
   IBSC->AddNode(IBBC,1,new TGeoTranslation(0,1.685,0));
   IBSC->AddNode(IBCC,1,new TGeoTranslation(20,0,0));
   IBSC->AddNode(IBCC,2,new TGeoTranslation(-20,0,0));
  CAVE->AddNode(IBSC,2,new TGeoCombiTrans(0,-23.6,-583.005,GetRot("180R")));
  CAVE->AddNode(IBSD,1,new TGeoTranslation(0,-11.01,397.7));
  CAVE->AddNode(IBSD,2,new TGeoTranslation(0,-11.01,442.8));
  CAVE->AddNode(IBSD,3,new TGeoCombiTrans(0,-11.01,-399.605,GetRot("180R")));
  CAVE->AddNode(IBSD,4,new TGeoCombiTrans(0,-11.01,-444.705,GetRot("180R")));
  CAVE->AddNode(IBSD,5,new TGeoTranslation(0,-7.2,397.7));
  CAVE->AddNode(IBSD,6,new TGeoTranslation(0,-7.2,442.8));
  CAVE->AddNode(IBSD,7,new TGeoCombiTrans(0,-7.2,-399.605,GetRot("180R")));
  CAVE->AddNode(IBSD,8,new TGeoCombiTrans(0,-7.2,-444.705,GetRot("180R")));
  CAVE->AddNode(IBSE,1,new TGeoTranslation(6.99,-13.44,397.7));
  CAVE->AddNode(IBSE,2,new TGeoTranslation(-6.99,-13.44,397.7));
  CAVE->AddNode(IBSE,3,new TGeoTranslation(6.99,-13.44,442.8));
  CAVE->AddNode(IBSE,4,new TGeoTranslation(-6.99,-13.44,442.8));
  CAVE->AddNode(IBSE,5,new TGeoTranslation(6.99,-13.44,-399.605));
  CAVE->AddNode(IBSE,6,new TGeoTranslation(-6.99,-13.44,-399.605));
  CAVE->AddNode(IBSE,7,new TGeoTranslation(6.99,-13.44,-444.705));
  CAVE->AddNode(IBSE,8,new TGeoTranslation(-6.99,-13.44,-444.705));
  CAVE->AddNode(IBSF,1,new TGeoCombiTrans(0,-16.51,411.7,GetRot("90YX")));
  CAVE->AddNode(IBSF,2,new TGeoCombiTrans(0,-16.51,-413.605,GetRot("90YX")));
  CAVE->AddNode(IBSG,1,new TGeoCombiTrans(0,-17.57,395.16,GetRot("D180")));
  CAVE->AddNode(IBSG,2,new TGeoCombiTrans(0,-17.57,400.24,GetRot("D180")));
  CAVE->AddNode(IBSG,3,new TGeoCombiTrans(0,-17.57,440.26,GetRot("D180")));
  CAVE->AddNode(IBSG,4,new TGeoCombiTrans(0,-17.57,445.34,GetRot("D180")));
  CAVE->AddNode(IBSG,5,new TGeoCombiTrans(0,-17.57,-397.065,GetRot("D180")));
  CAVE->AddNode(IBSG,6,new TGeoCombiTrans(0,-17.57,-402.145,GetRot("D180")));
  CAVE->AddNode(IBSG,7,new TGeoCombiTrans(0,-17.57,-442.165,GetRot("D180")));
  CAVE->AddNode(IBSG,8,new TGeoCombiTrans(0,-17.57,-447.245,GetRot("D180")));
  CAVE->AddNode(IBSH,1,new TGeoCombiTrans(6.35,0,397.7,GetRot("90YX")));
  CAVE->AddNode(IBSH,2,new TGeoCombiTrans(-6.35,0,397.7,GetRot("90YX")));
  CAVE->AddNode(IBSH,3,new TGeoCombiTrans(6.35,0,-399.605,GetRot("90YX")));
  CAVE->AddNode(IBSH,4,new TGeoCombiTrans(-6.35,0,-399.605,GetRot("90YX")));
  CAVE->AddNode(IBSH,5,new TGeoCombiTrans(7.62,0,442.8,GetRot("90YX")));
  CAVE->AddNode(IBSH,6,new TGeoCombiTrans(-7.62,0,442.8,GetRot("90YX")));
  CAVE->AddNode(IBSH,7,new TGeoCombiTrans(7.62,0,-444.705,GetRot("90YX")));
  CAVE->AddNode(IBSH,8,new TGeoCombiTrans(-7.62,0,-444.705,GetRot("90YX")));
}
